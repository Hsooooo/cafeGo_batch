<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="openPush">
	
	<!-- 전용망 전문 전송을 위한 트랜잭션ID 생성 -->
	<select id="openPush.getTransactionId" resultClass="java.lang.String">
		SELECT TO_CHAR(SYSDATE, 'YYMMDD') || LPAD(SE_MSR_EDT_TRANS_ID.NEXTVAL, 6, '0')
  		  FROM DUAL	
	</select>
	
</sqlMap>