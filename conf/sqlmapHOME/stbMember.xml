<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="member">
	<!-- 회원 휴대폰번호 조회 -->
	<select id="stbMember.getMemberPhone" parameterClass="java.lang.String" resultClass="java.lang.String">
		SELECT xx1.dec_varchar2_sel (phone, 10, 'PHONE') AS phone
		  FROM stb_member##
		 WHERE user_id = #userId#	
	</select>
		
</sqlMap>